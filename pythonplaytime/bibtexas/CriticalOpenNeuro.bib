@article{Becamel2004,
  title = {The {Serotonin} 5-{HT}2{A} and 5-{HT}2{C} {Receptors} {Interact} with {Specific} {Sets} of {PDZ} {Proteins}},
  volume = {279},
  issn = {0021-9258, 1083-351X},
  doi = {10.1074/jbc.M312106200},
  abstract = {The 5-hydroxytryptamine type 2A (5-HT2A) receptor and the 5-HT2C receptor are closely related members of the G-protein-coupled receptors activated by serotonin that share very similar pharmacological profiles and cellular signaling pathways. These receptors express a canonical class I PDZ ligand (SXV) at their C-terminal extremity. Here, we have identified proteins that interact with the PDZ ligand of the 5-HT2A and 5-HT2C receptors by a proteomic approach associating affinity chromatography using immobilized synthetic peptides encompassing the PDZ ligand and mass spectrometry. We report that both receptor C termini interact with specific sets of PDZ proteins in vitro. The 5-HT2C receptor but not the 5-HT2A receptor binds to the Veli-3\ensuremath{{\cdot}}CASK\ensuremath{{\cdot}}Mint1 ternary complex and to SAP102. In addition, the 5-HT2C receptor binds more strongly to PSD-95 and MPP-3 than the 5-HT2A receptor. In contrast, a robust interaction between the 5-HT2A receptor and the channel-interacting PDZ protein CIPP was found, whereas CIPP did not significantly associate with the 5-HT2C receptor. We also show that residues located at the -1 position and upstream the PDZ ligand in the C terminus of the 5-HT2A and 5-HT2C receptors are major determinants in their interaction with specific PDZ proteins. Immunofluorescence and electron microscopy studies strongly suggested that these specific interactions also take place in living cells and that the 5-HT2 receptor-PDZ protein complexes occur in intracellular compartments. The interaction of the 5-HT2A and the 5-HT2C receptor with specific sets of PDZ proteins may contribute to their different signal transduction properties.},
  language = {en},
  number = {19},
  urldate = {2015-01-07},
  journal = {Journal of {Biological} {Chemistry}},
  author = {B\'{e}camel, Carine and Gavarini, Sophie and Chanrion, Benjamin and Alonso, G\'{e}rard and Gal\'{e}otti, Nathalie and Dumuis, Aline and Bockaert, Jo\"{e}l and Marin, Philippe},
  month = may,
  year = {2004},
  note = {PMID: 14988405},
  pages = {20257--20266},
  file = {Bécamel et al_2004_The Serotonin 5-HT2A and 5-HT2C Receptors Interact with Specific Sets of PDZ.pdf:/home/feline/Documents/div3/resources/Bécamel et al_2004_The Serotonin 5-HT2A and 5-HT2C Receptors Interact with Specific Sets of PDZ.pdf:application/pdf;Snapshot:/home/feline/Documents/Zotero Files/storage/DJ6T43V5/20257.html:text/html}
}

@article{Mahaut-Smith2008,
  title = {A role for membrane potential in regulating {GPCRs}?},
  volume = {29},
  issn = {0165-6147},
  doi = {10.1016/j.tips.2008.05.007},
  abstract = {G-protein-coupled receptors (GPCRs) have ubiquitous roles in transducing extracellular signals into cellular responses. Therefore, the concept that members of this superfamily of surface proteins are directly modulated by changes in membrane voltage could have widespread consequences for cell signalling. Although several studies have indicated that GPCRs can be voltage dependent, particularly P2Y1 receptors in the non-excitable megakaryocyte, the evidence has been mostly indirect. Recent work on muscarinic receptors has stimulated substantial interest in this field by reporting the first voltage-dependent charge movements for a GPCR. An underlying mechanism is proposed whereby a voltage-induced conformational change in the receptor alters its ability to couple to the G protein and thereby influences its affinity for an agonist. We discuss the strength of the evidence behind this hypothesis and include suggestions for future work. We also describe other examples in which direct voltage control of GPCRs can account for effects of membrane potential on downstream signals and highlight the possible physiological consequences of this phenomenon.},
  number = {8},
  urldate = {2015-01-12},
  journal = {Trends in {Pharmacological} {Sciences}},
  author = {Mahaut-Smith, Martyn P. and Martinez-Pinna, Juan and Gurung, Iman S.},
  month = aug,
  year = {2008},
  pages = {421--429},
  file = {Mahaut-Smith_et-al-2008-A_role_for_membrane_potential_in_regulating_GPCRs.pdf:/home/feline/Documents/div3/resources/Mahaut-Smith_et-al-2008-A_role_for_membrane_potential_in_regulating_GPCRs2.pdf:application/pdf;ScienceDirect Snapshot:/home/feline/Documents/Zotero Files/storage/2D3RBPUN/S0165614708001454.html:text/html}
}

