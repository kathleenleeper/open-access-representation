@article{Gray2001,
  title = {Cell-{Type} {Specific} {Effects} of {Endocytosis} {Inhibitors} on 5-{Hydroxytryptamine}2{A} {Receptor} {Desensitization} and {Resensitization} {Reveal} an {Arrestin}-, {GRK}2-, and {GRK}5-{Independent} {Mode} of {Regulation} in {Human} {Embryonic} {Kidney} 293 {Cells}},
  volume = {60},
  issn = {, 1521-0111 (Online)},
  doi = {10.1124/mol.60.5.1020},
  abstract = {The effect of endocytosis inhibitors on 5-hydroxytryptamine2A (5-HT2A) receptor desensitization and resensitization was examined in transiently transfected human embryonic kidney (HEK) 293 cells and in C6 glioma cells that endogenously express 5-HT2A receptors. In HEK-293 cells, 5-HT2A receptor desensitization was unaffected by cotransfection with a dominant-negative mutant of dynamin (DynK44A), a truncation mutant of arrestin-2 {[}Arr2(319{\textendash}418){]}, or by two well-characterized chemical inhibitors of endocytosis: concanavalin A (conA) and phenylarsine oxide (PAO). In contrast, \ensuremath{{\beta}}2-adrenergic receptor desensitization was significantly potentiated by each of these treatments in HEK-293 cells. In C6 glioma cells, however, DynK44A, Arr2(319{\textendash}418), conA, and PAO each resulted in the potentiation of 5-HT2A and \ensuremath{{\beta}}-adrenergic receptor desensitization. The cell-type-specific effect of Arr2(319{\textendash}418) on 5-HT2Areceptor desensitization was not related to the level of GRK2 or GRK5 expression. Interestingly, although \ensuremath{{\beta}}2-adrenergic receptor resensitization was potently blocked by cotransfection with DynK44A, 5-HT2A receptor resensitization was enhanced, suggesting the existence of a novel cell-surface mechanism for 5-HT2Areceptor resensitization in HEK-293 cells. In addition, Arr2(319{\textendash}418) had no effect on 5-HT2A receptor resensitization in HEK-293 cells, although it attenuated the resensitization of the \ensuremath{{\beta}}2-adrenergic receptor. However, in C6 glioma cells, both DynK44A and Arr2(319{\textendash}418) significantly reduced 5-HT2A receptor resensitization. Taken together, these results provide the first convincing evidence of cell-type-specific roles for endocytosis inhibitors in regulating GPCR activity. Additionally, these results imply that novel GRK and arrestin-independent mechanisms of 5-HT2A receptor desensitization and resensitization exist in HEK-293 cells.},
  language = {en},
  number = {5},
  urldate = {2015-01-22},
  journal = {Molecular {Pharmacology}},
  author = {Gray, John A. and Sheffler, Douglas J. and Bhatnagar, Anushree and Woods, Jason A. and Hufeisen, Sandra J. and Benovic, Jeffrey L. and Roth, Bryan L.},
  month = nov,
  year = {2001},
  note = {PMID: 11641430},
  pages = {1020--1030},
  file = {Gray_et-al-2001-Cell-Type_Specific_Effects_of_Endocytosis.pdf:/home/feline/Documents/div3/resources/Gray_et-al-2001-Cell-Type_Specific_Effects_of_Endocytosis.pdf:application/pdf;Snapshot:/home/feline/Documents/Zotero Files/storage/CMQKEK2N/1020.html:text/html}
}

@article{Schmid2008,
  title = {Agonist-directed signaling of the serotonin 2{A} receptor depends on \ensuremath{{\beta}}-arrestin-2 interactions in vivo},
  volume = {105},
  issn = {0027-8424},
  doi = {10.1073/pnas.0708862105},
  abstract = {Visual and auditory hallucinations accompany certain neuropsychiatric disorders, such as schizophrenia, and they also can be induced by the use or abuse of certain drugs. The heptahelical serotonin 2A receptors (5-HT2ARs) are molecular targets for drug-induced hallucinations. However, the cellular mechanisms by which the 5-HT2AR mediates these effects are not well understood. Drugs acting at the 5-HT2AR can trigger diverse signaling pathways that may be directed by the chemical properties of the drug. \ensuremath{{\beta}}-arrestins are intracellular proteins that bind to heptahelical receptors and represent a point where such divergences in ligand-directed functional signaling could occur. Here we compare the endogenous agonist, serotonin, to a synthetic 5-HT2AR hallucinogenic agonist, 2,5-dimethoxy-4-iodoamphetamine (DOI), in mice lacking \ensuremath{{\beta}}-arrestin-2, as well as in cells lacking \ensuremath{{\beta}}-arrestins. In mice, we find that serotonin induces a head twitch response by a \ensuremath{{\beta}}-arrestin-2-dependent mechanism. However, DOI invokes the behavior independent of \ensuremath{{\beta}}-arrestin-2. The two structurally distinct agonists elicit different signal transduction and trafficking patterns upon activation of 5-HT2AR, which hinge on the presence of \ensuremath{{\beta}}-arrestins. Our study suggests that the 5-HT2AR{\textendash}\ensuremath{{\beta}}-arrestin interaction may be particularly important in receptor function in response to endogenous serotonin levels, which could have major implications in drug development for treating neuropsychiatric disorders such as depression and schizophrenia.},
  number = {3},
  urldate = {2015-01-22},
  journal = {Proceedings of the {National} {Academy} of {Sciences} of the {United} {States} of {America}},
  author = {Schmid, Cullen L. and Raehal, Kirsten M. and Bohn, Laura M.},
  month = jan,
  year = {2008},
  note = {PMID: 18195357
PMCID: PMC2242710},
  keywords = {5-HT2A receptor,G protein-coupled receptor,internalization,MAP kinase,Schizophrenia},
  pages = {1079--1084},
  file = {Schmid_et-al-2008-Agonist-directed_signaling_of_the_serotonin_2A.pdf:/home/feline/Documents/div3/resources/Schmid_et-al-2008-Agonist-directed_signaling_of_the_serotonin_2A.pdf:application/pdf;Schmid_et-al-2008-Agonist-directed_signaling_of_the_serotonin_2A.pdf:/home/feline/Documents/div3/resources/Schmid_et-al-2008-Agonist-directed_signaling_of_the_serotonin_2A2.pdf:application/pdf;Snapshot:/home/feline/Documents/Zotero Files/storage/8GWT2VIG/1079.html:text/html}
}

