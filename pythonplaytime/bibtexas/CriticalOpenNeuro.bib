@article{Kanagarajadurai2009,
  title = {Molecular modeling and docking studies of human 5-hydroxytryptamine 2{A} (5-{HT}2{A}) receptor for the identification of hotspots for ligand binding},
  volume = {5},
  issn = {1742-2051},
  doi = {10.1039/B906391A},
  abstract = {The serotonergic system has been implicated in emotional and cognitive function. In particular, 5-HT2A (5-hydroxytrytamine receptor 2A) is attributed to a number of disorders like schizophrenia, depression, eating disorders and anxiety. 5-HT2A, being a GPCR (G-protein coupled receptor), is important in the pharmaceutical industry as a proven target for these disorders. Despite their extensive clinical importance, the structural studies of this protein is lacking due to difficulties in determining its crystal structure. We have performed sequence analysis and molecular modeling of 5-HT2A that has revealed a set of conserved residues and motifs considered to play an important role in maintaining structural integrity and function of the receptor. The analysis also revealed a set of residues specific to the receptor which distinguishes them from other members of the subclass and their orthologs. Further, starting from the model structure of human 5-HT2A receptor, docking studies were attempted to envisage how it might interact with eight of its ligands (such as serotonin, dopamine, DOI, LSD, haloperidol, ketanserin, risperidone and clozapine). The binding studies of dopamine to 5-HT2A receptor can bring up better understanding in the etiology of a number of neurological disorders involving both these two receptors. Our sequence analysis and study of interactions of this receptor with other ligands reveal additional residue hotspots such as Asn 363 and Tyr 370. The function of these residues can be further analyzed by rational design of site-directed mutagenesis. Two distinct binding sites are identified which could play important roles in ligand binding and signaling.},
  language = {en},
  number = {12},
  urldate = {2015-01-29},
  journal = {Molecular {BioSystems}},
  author = {Kanagarajadurai, Karuppiah and Malini, Manoharan and Bhattacharya, Aditi and Panicker, Mitradas M. and Sowdhamini, Ramanathan},
  month = nov,
  year = {2009},
  pages = {1877--1888},
  file = {Kanagarajadurai_et-al-2009-Molecular_modeling_and_docking_studies_of_human.pdf:/home/feline/Documents/div3/resources/Kanagarajadurai_et-al-2009-Molecular_modeling_and_docking_studies_of_human.pdf:application/pdf;Snapshot:/home/feline/Documents/Zotero Files/storage/R7QXZ5SF/b906391a.html:text/html}
}

@article{Scott2001,
  title = {Evidence that a protein{\textendash}protein interaction `hot spot' on heterotrimeric {G} protein \ensuremath{{\beta}{\gamma}} subunits is used for recognition of a subclass of effectors},
  volume = {20},
  issn = {0261-4189},
  doi = {10.1093/emboj/20.4.767},
  abstract = {To understand the requirements for binding to G protein \ensuremath{{\beta}{\gamma}} subunits, phage-displayed random peptide libraries were screened using immobilized biotinylated \ensuremath{{\beta}{\gamma}} as the target. Selected peptides were grouped into four different families based on their sequence characteristics. One group (group I) had a clear conserved motif that has significant homology to peptides derived from phospholipase C \ensuremath{{\beta}} (PLC \ensuremath{{\beta}}) and to a short motif in phosducin that binds to G protein \ensuremath{{\beta}} subunits. The other groups had weaker sequence homologies or no homology to the group I sequences. A synthetic peptide from the strongest consensus group blocked activation of PLC by G protein \ensuremath{{\beta}{\gamma}} subunits. The peptide did not block \ensuremath{{\beta}{\gamma}}-mediated inhibition of voltage-gated calcium channels and had little effect on \ensuremath{{\beta}{\gamma}}-mediated inhibition of Gs-stimulated type I adenylate cyclase. Competition experiments indicated that peptides from all four families bound to a single site on \ensuremath{{\beta}{\gamma}}. These peptides may bind to a protein{\textendash}protein interaction `hot spot' on the surface of \ensuremath{{\beta}{\gamma}} subunits that is used by a subclass of effectors.},
  number = {4},
  urldate = {2015-01-23},
  journal = {The {EMBO} {Journal}},
  author = {Scott, Jamie K. and Huang, Shan-Fu and Gangadhar, Beechanahalli P. and Samoriski, Gary M. and Clapp, Peter and Gross, Robert A. and Taussig, Ronald and Smrcka, Alan V.},
  month = feb,
  year = {2001},
  note = {PMID: 11179221
PMCID: PMC145424},
  pages = {767--776},
  file = {PubMed Central Full Text PDF:/home/feline/Documents/Zotero Files/storage/8NWMJWFR/Scott et al. - 2001 - Evidence that a protein–protein interaction ‘hot s.pdf:application/pdf;Scott_et-al-2001-Evidence_that_a_protein–protein_interaction_‘hot.pdf:/home/feline/Documents/div3/resources/Scott_et-al-2001-Evidence_that_a_protein–protein_interaction_‘hot.pdf:application/pdf}
