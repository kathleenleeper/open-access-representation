@article{Jager2014,
  title = {An estimate of the science-wise false discovery rate and application to the top medical literature},
  volume = {15},
  issn = {1465-4644, 1468-4357},
  doi = {10.1093/biostatistics/kxt007},
  abstract = {The accuracy of published medical research is critical for scientists, physicians and patients who rely on these results. However, the fundamental belief in the medical literature was called into serious question by a paper suggesting that most published medical research is false. Here we adapt estimation methods from the genomics community to the problem of estimating the rate of false discoveries in the medical literature using reported PP-values as the data. We then collect PP-values from the abstracts of all 77 430 papers published in The Lancet, The Journal of the American Medical Association, The New England Journal of Medicine, The British Medical Journal, and The American Journal of Epidemiology between 2000 and 2010. Among these papers, we found 5322 reported PP-values. We estimate that the overall rate of false discoveries among reported results is 14\% (s.d. 1\%), contrary to previous claims. We also found that there is no a significant increase in the estimated rate of reported false discovery results over time (0.5\% more false positives (FP) per year, P=0.18P = 0.18) or with respect to journal submissions (0.5\% more FP per 100 submissions, P=0.12P = 0.12). Statistical analysis must allow for false discoveries in order to make claims on the basis of noisy data. But our analysis suggests that the medical literature remains a reliable record of scientific progress.},
  language = {en},
  number = {1},
  urldate = {2015-01-15},
  journal = {Biostatistics},
  author = {Jager, Leah R. and Leek, Jeffrey T.},
  month = jan,
  year = {2014},
  note = {PMID: 24068246},
  keywords = {False discovery rate,Genomics,meta-analysis,Multiple testing,Science-wise false discovery rate,Two-group model},
  pages = {1--12},
  file = {Jager_Leek_2014_An estimate of the science-wise false discovery rate and application to the top.pdf:/home/feline/Documents/div3/resources/Jager_Leek_2014_An estimate of the science-wise false discovery rate and application to the top.pdf:application/pdf;Snapshot:/home/feline/Documents/Zotero Files/storage/GD76FCJD/1.html:text/html}
}

@article{Ioannidis2014,
  title = {Discussion: {Why} ``{An} estimate of the science-wise false discovery rate and application to the top medical literature'' is false},
  volume = {15},
  issn = {1465-4644, 1468-4357},
  shorttitle = {Discussion},
  doi = {10.1093/biostatistics/kxt036},
  abstract = {Jager and Leek have tried to estimate a false-discovery rate (FDR) in abstracts of articles published in five medical journals during 2000{\textendash}2010. Their approach is flawed in sampling, calculations, and conclusions. It uses a tiny portion of select papers in highly select journals. Randomized controlled trials and systematic reviews (designs with the lowest anticipated false-positive rates) are 52\% of the analyzed papers, while these designs account for only 4\% in PubMed in the same period. The FDR calculations consider the entire published literature as equivalent to a single genomic experiment where all performed analyses are reported without selection or distortion. However, the data used are the P-values reported in the abstracts of published papers; these P-values are a highly distorted, highly select sample. Besides selective reporting biases, all other biases, in particular confounding in observational studies, are also ignored, while these are often the main drivers for high false-positive rates in the biomedical literature. A reproducibility check of the raw data shows that much of the data Jager and Leek used are either wrong or make no sense: most of the usable data were missed by their script, 94\% of the abstracts that reported \ensuremath{{\geq}}2 P-values had high correlation/overlap between reported outcomes, and only a minority of P-values corresponded to relevant primary outcomes. The Jager and Leek paper exemplifies the dreadful combination of using automated scripts with wrong methods and unreliable data. Sadly, this combination is common in the medical literature.},
  language = {en},
  number = {1},
  urldate = {2015-01-15},
  journal = {Biostatistics},
  author = {Ioannidis, John P. A.},
  month = jan,
  year = {2014},
  note = {PMID: 24068251},
  keywords = {Bias,False discovery rate,P-value,science,Selection bias},
  pages = {28--36},
  file = {Ioannidis_2014_Discussion.pdf:/home/feline/Documents/div3/resources/Ioannidis_2014_Discussion.pdf:application/pdf;Ioannidis-2014-Discussion.pdf:/home/feline/Documents/div3/resources/Ioannidis-2014-Discussion.pdf:application/pdf;Snapshot:/home/feline/Documents/Zotero Files/storage/FZDADW2Z/28.html:text/html;Snapshot:/home/feline/Documents/Zotero Files/storage/R5CP6WCP/28.html:text/html}
