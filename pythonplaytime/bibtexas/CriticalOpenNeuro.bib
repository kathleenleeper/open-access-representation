@article{Violin2007,
  series = {Special {Issue} on {Allosterism} and {Collateral} {Efficacy}},
  title = {\ensuremath{{\beta}}-{Arrestin}-biased ligands at seven-transmembrane receptors},
  volume = {28},
  issn = {0165-6147},
  doi = {10.1016/j.tips.2007.06.006},
  abstract = {Seven-transmembrane receptors (7TMRs), the most common molecular targets of modern drug therapy, are critically regulated by \ensuremath{{\beta}}-arrestins, which both inhibit classic G-protein signaling and initiate distinct \ensuremath{{\beta}}-arrestin signaling. The interplay of G-protein and \ensuremath{{\beta}}-arrestin signals largely determines the cellular consequences of 7TMR-targeted drugs. Until recently, a drug's efficacy for \ensuremath{{\beta}}-arrestin recruitment was believed to be proportional to its efficacy for G-protein activities. This paradigm restricts 7TMR drug effects to a linear spectrum of responses, ranging from inhibition of all responses to stimulation of all responses. However, it is now clear that `biased ligands' can selectively activate G-protein or \ensuremath{{\beta}}-arrestin functions and thus elicit novel biological effects from even well-studied 7TMRs. Here, we discuss the current state of \ensuremath{{\beta}}-arrestin-biased ligand research and the prospects for \ensuremath{{\beta}}-arrestin bias as a therapeutic target. Consideration of ligand bias might have profound influences on the way scientists approach 7TMR-targeted drug discovery.},
  number = {8},
  urldate = {2015-01-22},
  journal = {Trends in {Pharmacological} {Sciences}},
  author = {Violin, Jonathan D. and Lefkowitz, Robert J.},
  month = aug,
  year = {2007},
  pages = {416--422},
  file = {Violin-Lefkowitz-2007-β-Arrestin-biased_ligands_at_seven-transmembrane.pdf:/home/feline/Documents/div3/resources/Violin-Lefkowitz-2007-β-Arrestin-biased_ligands_at_seven-transmembrane.pdf:application/pdf}
}

