  volume = {323},
  issn = {, 1521-0103 (Online)},
  doi = {10.1124/jpet.107.122317},
  abstract = {We previously demonstrated that 24-h treatment with ({\textendash})-1-(2,5-dimethoxy-4-iodophenyl)-2-aminopropane HCl (DOI) causes phosphorylation of G\ensuremath{{\alpha}}11 protein at serine 154 and that this phosphorylation causes desensitization of serotonin (5-HT) 2A receptor signaling in A1A1v cells (Shi et al., 2007). We now report that treatment of A1A1v cells with DOI for 24 h produces a greater reduction in the Bmax of {[}125I{]}(\ensuremath{{\pm}})-DOI-labeled high-affinity binding sites (46\%) than the reduction of {[}3H{]}ketanserin binding sites (25\%). Although the KD values are not altered, there is a smaller amount of GTP\ensuremath{{\gamma}}S {[}guanosine 5\ensuremath{{'}}-3-O-(thio)triphosphate{]}-sensitive {[}125I{]}(\ensuremath{{\pm}})-DOI binding in DOI-treated cells. These results suggest that DOI treatment causes down-regulation of 5-HT2A receptors and reductions in G protein-coupled 5-HT2A receptors. In contrast, in cells transfected with the phosphorylation state mimic G\ensuremath{{\alpha}}11S154D, GTP\ensuremath{{\gamma}}S-sensitive {[}125I{]}(\ensuremath{{\pm}})-DOI binding was decreased by 48\%; however, there was no significant difference in the KD and Bmax values of {[}125I{]}(\ensuremath{{\pm}})-DOI-labeled receptors. The receptor binding experiments suggest that phosphorylation of G\ensuremath{{\alpha}}11 on serine 154 reduces coupling of 5-HT2A receptors, whereas DOI causes down-regulation of 5-HT2A receptors in addition to the phosphorylation-induced uncoupling of G\ensuremath{{\alpha}}11 to 5-HT2A receptors. To determine whether DOI increases phosphorylation of G\ensuremath{{\alpha}}q/11 protein in vivo, rats were treated with 1 mg/kg/day DOI or saline for 1 to 7 days. Seven days of DOI treatment significantly decreased phospholipase C activity stimulated by an Emax concentration of 5-HT by 40\% and increased phosphorylation of G\ensuremath{{\alpha}}q/11 proteins by 51\% in the frontal cortex. These data suggest that DOI causes phosphorylation of G\ensuremath{{\alpha}}q/11 in vivo and could thereby contribute to the desensitization of 5-HT2A receptors.},
  language = {en},
  number = {1},
  urldate = {2015-01-23},
  journal = {Journal of {Pharmacology} and {Experimental} {Therapeutics}},
  author = {Shi, Ju and Damjanoska, Katerina J. and Singh, Rakesh K. and Carrasco, Gonzalo A. and Garcia, Francisca and Grippo, Angela J. and Landry, Michelle and Sullivan, Nicole R. and Battaglia, George and Muma, Nancy A.},
  month = oct,
  year = {2007},
  note = {PMID: 17646429},
  keywords = {Amphetamines,Animals,Cell Culture Techniques,Cell Line,Fluoxetine,Frontal Lobe,GTP-Binding Protein alpha Subunits, Gq-G11,Ketanserin,Male,Phosphorylation,Protein Binding,Rats,Rats, Sprague-Dawley,Receptor, Serotonin, 5-HT2A,Serotonin 5-HT2 Receptor Agonists,Serotonin Receptor Agonists,Serotonin Uptake Inhibitors,Signal Transduction,Transfection,Type C Phospholipases},
  pages = {248--256},
  file = {Full Text PDF:/home/feline/Documents/Zotero Files/storage/GQUHWJGE/Shi et al. - 2007 - Agonist Induced-Phosphorylation of Gα11 Protein Re.pdf:application/pdf;Shi_et-al-2007-Agonist_Induced-Phosphorylation_of_G_11_Protein.pdf:/home/feline/Documents/div3/resources/Shi_et-al-2007-Agonist_Induced-Phosphorylation_of_G_11_Protein.pdf:application/pdf;Snapshot:/home/feline/Documents/Zotero Files/storage/ZH3E4SCQ/248.html:text/html}
}

@article{Gelber1999,
  title = {Structure and {Function} of the {Third} {Intracellular} {Loop} of the 5-{Hydroxytryptamine}2{A} {Receptor}: {The} {Third} {Intracellular} {Loop} {Is} \ensuremath{{\alpha}}-{Helical} and {Binds} {Purified} {Arrestins}},
  volume = {72},
  issn = {1471-4159},
  shorttitle = {Structure and {Function} of the {Third} {Intracellular} {Loop} of the 5-{Hydroxytryptamine}2{A} {Receptor}},
  doi = {10.1046/j.1471-4159.1999.0722206.x},
  abstract = {Abstract:  Understanding the precise structure and function of the intracellular domains of G protein-coupled receptors is essential for understanding how receptors are regulated, and how they transduce their signals from the extracellular milieu to intracellular sites. To understand better the structure and function of the intracellular domain of the 5-hydroxytryptamine2A (5-HT2A) receptor, a model G\ensuremath{{\alpha}}q-coupled receptor, we overexpressed and purified to homogeneity the entire third intracellular loop (i3) of the 5-HT2A receptor, a region previously implicated in G-protein coupling. Circular dichroism spectroscopy of the purified i3 protein was consistent with \ensuremath{{\alpha}}-helical and \ensuremath{{\beta}}-loop, -turn, and -sheet structure. Using random peptide phage libraries, we identified several arrestin-like sequences as i3-interacting peptides. We subsequently found that all three known arrestins (\ensuremath{{\beta}}-arrestin, arrestin-3, and visual arrestin) bound specifically to fusion proteins encoding the i3 loop of the 5-HT2A receptor. Competition binding studies with synthetic and recombinant peptides showed that the middle portion of the i3 loop, and not the extreme N and C termini, was likely to be involved in i3-arrestin interactions. Dual-label immunofluorescence confocal microscopic studies of rat cortex indicated that many cortical pyramidal neurons coexpressed arrestins (\ensuremath{{\beta}}-arrestin or arrestin-3) and 5-HT2A receptors, particularly in intracellular vesicles. Our results demonstrate (a) that the i3 loop of the 5-HT2A receptor represents a structurally ordered domain composed of \ensuremath{{\alpha}}-helical and \ensuremath{{\beta}}-loop, -turn, and -sheet regions, (b) that this loop interacts with arrestins in vitro, and is hence active, and (c) that arrestins are colocalized with 5-HT2A receptors in vivo.},
  language = {en},
  number = {5},
  urldate = {2015-01-24},
  journal = {Journal of {Neurochemistry}},
  author = {Gelber, Edward I. and Kroeze, Wesley K. and Roth, Bryan L. and Gray, John A. and Sinar, Christine A. and Hyde, Edward G. and Gurevich, Vsevolod and Benovic, Jeffrey and Roth, Bryan L.},
  year = {1999},
  keywords = {5-HT2A receptor,Receptor-effector coupling,Structure-function,\ensuremath{{\beta}}-Arrestin},
  pages = {2206--2214},
  file = {Gelber_et-al-1999-Structure_and_Function_of_the_Third_Intracellular.pdf:/home/feline/Documents/div3/resources/Gelber_et-al-1999-Structure_and_Function_of_the_Third_Intracellular.pdf:application/pdf;Snapshot:/home/feline/Documents/Zotero Files/storage/2UDA35HT/abstract\;jsessionid=FC75376D612504A2001ACCCB279DDD16.html:text/html}
}

@article{Roth2008,
  title = {{INSIGHTS} {INTO} {THE} {REGULATION} {OF} 5-{HT}2{A} {RECEPTORS} {BY} {SCAFFOLDING} {PROTEINS} {AND} {KINASES}},
  volume = {55},
  issn = {0028-3908},
  doi = {10.1016/j.neuropharm.2008.06.048},
  abstract = {5-HT2A serotonin receptors are essential molecular targets for the actions of LSD-like hallucinogens and atypical antipsychotic drugs. 5-HT2A serotonin receptors also mediate a variety of physiological processes in peripheral and central nervous systems including platelet aggregation, smooth muscle contraction, and the modulation of mood and perception. Scaffolding proteins have emerged as important regulators of 5-HT2A receptors and our recent studies suggest multiple scaffolds exist for 5-HT2A receptors including PSD95, arrestin, and caveolin. In addition, a novel interaction has emerged between p90 ribosomal S6 kinase and 5-HT2A receptors which attenuates receptor signaling. This article reviews our recent studies and emphasizes the role of scaffolding proteins and kinases in the regulation of 5-HT2A trafficking, targeting and signaling.},
