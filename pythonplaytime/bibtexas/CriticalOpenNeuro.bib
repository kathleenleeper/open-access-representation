  file = {Snapshot:/home/feline/Documents/Zotero Files/storage/RGBW8QPM/1159.html:text/html;Wettschureck-Offermanns-2005-Mammalian_G_Proteins_and_Their_Cell_Type_Specific.pdf:/home/feline/Documents/div3/resources/Wettschureck-Offermanns-2005-Mammalian_G_Proteins_and_Their_Cell_Type_Specific.pdf:application/pdf}
